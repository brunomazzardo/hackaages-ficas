<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="ages.hacka.fichasapp.configuracaoSalaActivity">

    <TextView
        android:id="@+id/nomeSala"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Nome da Sala"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="16dp" />

    <TextView
        android:id="@+id/buyIn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Buy-in"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="53dp" />

    <TextView
        android:id="@+id/reBuy"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Re-Buy"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="90dp" />

    <Button
        android:id="@+id/criarSalaBtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Criar Sala"
        tools:layout_editor_absoluteX="148dp"
        tools:layout_editor_absoluteY="414dp" />

    <TextView
        android:id="@+id/valorFichas"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Valor em Fichas"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="128dp" />

</android.support.constraint.ConstraintLayout>
